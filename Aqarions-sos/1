def golden_ratio_consensus(responses, weights=None):
    if weights is None:
        # Default: geometric series with ratio Î¦â»Â¹
        weights = [PHI_INV**i for i in range(len(responses))]
    
    # Normalize weights
    weights = np.array(weights) / np.sum(weights)
    
    # Weighted synthesis
    consensus = weighted_merge(responses, weights)
    
    # Chaos audit
