services:

  spintronics:
    ruo2_py_igsc:
      repo: https://github.com/aqarion/Aqarions-SoS
      path: services/spintronics/ruo2_py_igsc
      type: spin-hall-interface-sim
      description: >
        RuO2/Py interface-generated spin currents (IGSC) and spin Hall
        nano-oscillator fields feeding membrane/ion-channel models.
      ports: [8210]
      healthEndpoint: /health
      config:
        theta_SH: 6.1e4           # effective spin Hall parameter (unitless model)
        defaultCurrentDensity: 1.0e8   # A/m^2
        backend: jax

    graphene_bridge:
      repo: https://github.com/aqarion/Aqarions-SoS
      path: services/spintronics/graphene_bridge
      type: spin-charge-bridge
      description: >
        Graphene/thiol bridge from spin voltage to surface charge at the
        membrane / HBG gel interface.

  ionics:
    hbg_gel_synapse:
      repo: https://github.com/aqarion/Aqarions-SoS
      path: services/ionics/hbg_gel_synapse
      type: iontronic-synapse-model
      description: >
        Hydrogel-based iontronic synapse model (HBG / PEDOT:PSS) translating
        spin-modulated voltages into ionic currents and effective AP-like pulses.
      ports: [8220]
      config:
        defaultTemperatureC: 37.0
        ionSpecies: [Na, K, Ca, Cl]
        timeStepMs: 0.1

  nvQuantumSensing:
    nv_membrane_film:
      repo: https://github.com/aqarion/Aqarions-SoS
      path: services/nv/nv_membrane_film
      type: nv-geometry-config
      description: >
        Geometry and calibration for shallow NV layer under supported membrane
        films (5–10 nm depth, in vitro single-channel NV setup).
      ports: [8230]
      healthEndpoint: /nv-status
      config:
        nvDepthNm: 7
        layerType: shallow-ensemble
        laserWavelengthNm: 532
        defaultB0_mT: 20

    single_channel_nv_readout:
      repo: https://github.com/aqarion/Aqarions-SoS
      path: services/nv/single_channel_nv_readout
      type: nv-relaxometry-analysis
      description: >
        NV T1/T2 relaxometry and Ramsey/Hahn-echo analysis to extract
        decoherence rates correlated with single ion-channel gating.
      config:
        protocols:
          - T1_relaxometry
          - Ramsey_lowPower
          - HahnEcho_lowDuty
        outputFields:
          - t
          - T1
          - T2
          - decoherenceRate
          - confidence

  ionChannels:
    nav_family:
      repo: https://github.com/aqarion/Aqarions-SoS
      path: models/ion_channels/nav
